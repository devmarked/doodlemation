{
	"buildCommand": "pnpm build",
	"devCommand": "pnpm dev",
	"installCommand": "pnpm install",
	"framework": "sveltekit",
	"regions": ["iad1"],
	"functions": {
		"api/**/*.ts": {
			"maxDuration": 60
		}
	},
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, POST, OPTIONS"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "Content-Type"
				}
			]
		}
	]
}

